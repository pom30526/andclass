<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">

	<LinearLayout android:layout_width="wrap_content"
		android:id="@+id/LinearLayout01" android:layout_height="wrap_content">
		<TextView android:text="위도" android:layout_width="wrap_content"
			android:layout_height="wrap_content" />
		<EditText android:text="" android:id="@+id/lattitude"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
		<TextView android:text="경도" android:layout_width="wrap_content"
			android:layout_height="wrap_content" />
		<EditText android:text="" android:id="@+id/longitude"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
		<Button android:text="변환" android:id="@+id/transform"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:onClick="onClick" />
	</LinearLayout>
	<LinearLayout android:id="@+id/LinearLayout02"
		android:layout_width="wrap_content" android:layout_height="wrap_content">
		<TextView android:text="주소" android:id="@+id/TextView01"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
		<EditText android:text="" android:id="@+id/address"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
		<Button android:text="변환" android:id="@+id/trasnform1"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:onClick="onClick1" />
	</LinearLayout>
	<TextView android:text="" android:id="@+id/result"
		android:layout_width="wrap_content" android:layout_height="wrap_content" />
</LinearLayout>
